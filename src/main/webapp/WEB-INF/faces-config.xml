<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>    
        
        <navigation-case>
            <from-outcome>painel_de_controle</from-outcome>
            <to-view-id>/painel_de_controle.xhtml</to-view-id>
            <redirect/>
        </navigation-case>  
          
        <navigation-case>
            <from-outcome>pessoa</from-outcome>
            <to-view-id>/pessoa.xhtml</to-view-id>
            <redirect/>
        </navigation-case>    
        
        <navigation-case>
            <from-outcome>lista_pessoa</from-outcome>
            <to-view-id>/lista_pessoa.xhtml</to-view-id>
            <redirect/>
        </navigation-case>    
        
        <navigation-case>
            <from-outcome>permissoes</from-outcome>
            <to-view-id>/permissoes.xhtml</to-view-id>
            <redirect/>
        </navigation-case>    
        
        <navigation-case>
            <from-outcome>entrada_pf</from-outcome>
            <to-view-id>/entrada_pf.xhtml</to-view-id>
            <redirect/>
        </navigation-case>    
    </navigation-rule>
    
</faces-config>
